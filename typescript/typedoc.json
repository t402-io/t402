{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": [
    "packages/core/src/index.ts",
    "packages/extensions/src/index.ts",
    "packages/mechanisms/evm/src/index.ts",
    "packages/mechanisms/svm/src/index.ts",
    "packages/mechanisms/ton/src/index.ts",
    "packages/mechanisms/tron/src/index.ts",
    "packages/http/express/src/index.ts",
    "packages/http/next/src/index.ts",
    "packages/http/hono/src/index.ts",
    "packages/http/fastify/src/index.ts",
    "packages/http/fetch/src/index.ts",
    "packages/http/axios/src/index.ts",
    "packages/http/paywall/src/index.ts",
    "packages/http/react/src/index.ts",
    "packages/http/vue/src/index.ts",
    "packages/wdk/src/index.ts",
    "packages/wdk-gasless/src/index.ts",
    "packages/wdk-bridge/src/index.ts",
    "packages/wdk-multisig/src/index.ts",
    "packages/mcp/src/index.ts",
    "packages/cli/src/index.ts"
  ],
  "entryPointStrategy": "resolve",
  "out": "api-docs",
  "name": "T402 API Reference",
  "readme": "../README.md",
  "skipErrorChecking": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeInternal": true,
  "excludeExternals": true,
  "exclude": [
    "**/node_modules/**",
    "**/dist/**",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/test/**",
    "**/bench/**"
  ],
  "includeVersion": true,
  "navigationLinks": {
    "Website": "https://t402.io",
    "Documentation": "https://docs.t402.io",
    "GitHub": "https://github.com/t402-io/t402"
  },
  "categorizeByGroup": true,
  "categoryOrder": [
    "Core",
    "Mechanisms",
    "HTTP",
    "WDK",
    "Tools",
    "*"
  ],
  "sort": ["source-order", "alphabetical"],
  "validation": {
    "notExported": false,
    "invalidLink": false,
    "notDocumented": false
  },
  "searchInComments": true,
  "searchInDocuments": true
}
